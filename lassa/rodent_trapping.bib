	keywords = {Animals, Humans, Disease Reservoirs, Adolescent, Adult, Female, Male, Middle Aged, Sierra Leone, Child, Child, Preschool, Lassa virus/im [Immunology], Prospective Studies, *Lassa Fever/ep [Epidemiology], Antibodies, Viral/an [Analysis], Lassa Fever/tm [Transmission], Rodent Diseases/ep [Epidemiology], Lassa Fever/ve [Veterinary], Muridae/mi [Microbiology]},
	file = {McCormick et al. - 1987 - A Prospective Study of the Epidemiology and Ecolog.pdf:C\:\\Users\\ucbtds4\\Zotero\\storage\\4JMGJA9Y\\McCormick et al. - 1987 - A Prospective Study of the Epidemiology and Ecolog.pdf:application/pdf},
