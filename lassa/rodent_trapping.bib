	langid = {english},
	keywords = {Agricultural expansion, Ashanti region of Ghana, Deforestation, Farm intensification, Forest transition, Multi-functional forest landscapes},
	file = {Accepted Version:C\:\\Users\\ucbtds4\\Zotero\\storage\\AQI3FLNX\\Acheampong et al. - 2019 - Deforestation is driven by agricultural expansion .pdf:application/pdf},
