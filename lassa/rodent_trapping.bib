	pmid = {19687045},
	pmcid = {PMC2865085},
	keywords = {burden of disease, endemic zoonoses, poverty, under-reporting},
	file = {Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\RW2Y5BP9\\Maudlin et al. - 2009 - Neglected and endemic zoonoses.pdf:application/pdf;Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\DZAFQ4JM\\Maudlin et al. - 2009 - Neglected and endemic zoonoses.pdf:application/pdf},
