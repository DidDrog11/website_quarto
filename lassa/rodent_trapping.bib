	langid = {english},
	note = {Publisher: {SAGE} Publications Ltd},
	keywords = {infectious disease, demographic change, extended urbanisation, governance, infrastructure, urban political ecology, 人口变化, 城市政治生态, 基础设施, 扩展城市化, 治理、传染病},
	file = {SAGE PDF Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\GRJUTXBQ\\Connolly et al. - 2021 - Extended urbanisation and the spatialities of infe.pdf:application/pdf;SAGE PDF Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\HKLUBQVN\\Connolly et al. - 2021 - Extended urbanisation and the spatialities of infe.pdf:application/pdf},
