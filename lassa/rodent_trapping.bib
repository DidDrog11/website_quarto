	note = {\_eprint: https://besjournals.onlinelibrary.wiley.com/doi/pdf/10.1002/2688-8319.12033},
	keywords = {human rights, camera trap, code of conduct, ethics, law, {PARTNERS} principles for community-based conservation, privacy, snow leopard},
