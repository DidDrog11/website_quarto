	urldate = {2022-03-06},
	date = {2020-01-08},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\PUAAXDCF\\Urban et al. - 2020 - PHI-base the pathogen–host interactions database.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\L8LK2PYD\\5626528.html:text/html},
