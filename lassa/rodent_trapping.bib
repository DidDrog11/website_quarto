@article{leirs_productivity_1993,
	title = {Productivity of Different Generations in a Population of Mastomys natalensis Rats in Tanzania},
	volume = {68},
	issn = {0030-1299},
	url = {https://www.jstor.org/stable/3545308},
	doi = {10.2307/3545308},
	abstract = {In Tanzania three different generation types can be recognized in Mastomys natalensis rats. The α-generation is born in the main breeding season and breeds in the next main breeding season. The β-generation is born in the main breeding season but reproduces already after abundant off-season rainfall. Its offspring, the γ-generation, is born in the off-season breeding period and breeds in the same year. Life histories of these three generation types are different. Generation α has a long growth stop, poor survival until breeding but up to six consecutive litters; generation β has a short growth stop, a better survival until reproduction but breeds only once or twice. The γ-generation, however, grows very fast and reproduces at young age. Through their daughters, the β-generation thus produces more than seven times as much young as the α-generation before the end of the following year. The occurrence of these generation types partially explains the relationship between rainfall patterns and population fluctuations.},
	pages = {53--60},
	number = {1},
	journaltitle = {Oikos},
	author = {Leirs, Herwig and Verhagen, Ron and Verheyen, Walter},
	urldate = {2022-02-22},
